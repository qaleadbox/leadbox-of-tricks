{
  "manifest_version": 3,
  "name": "Crawl Inventory Checker",
  "version": "1.1",
  "description": "Script to find images load problems.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
